{
  "name": "parcel-react-static-starter",
  "private": true,
  "version": "0.0.0",
  "targets": {
    "react-static": {
      "source": "src/pages/*",
      "context": "react-server"
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "predeploy": "rm -rf .parcel-cache && rm -rf dist && parcel build --public-url ./",
    "deploy": "gh-pages -d dist --nojekyll --cname matthewhua.ng"
  },
  "dependencies": {
    "@parcel/rsc": "^2.14.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@parcel/packager-react-static": "^2.16.0",
    "@parcel/transformer-react-static": "^2.16.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "gh-pages": "^6.3.0",
    "parcel": "^2.14.0"
  }
}
